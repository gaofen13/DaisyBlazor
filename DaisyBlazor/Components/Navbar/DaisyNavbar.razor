@namespace DaisyBlazor
@inherits DaisyComponentBase

<div class="@Classname" style="@Style" @attributes="@AdditionalAttributes">
    @if (ChildContent is not null)
    {
        @ChildContent
    }
    else
    {
        <div class="navbar-start">@StartContent</div>
        <div class="navbar-center">@CenterContent</div>
        <div class="navbar-end">@EndContent</div>
    }
</div>
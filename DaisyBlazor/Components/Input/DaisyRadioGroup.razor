@namespace DaisyBlazor
@inherits DaisyInputBase<TValue>
@typeparam TValue

<div @ref="@Element" id="@Id" style="width:100%" @attributes="@Attributes">
    <div role="radiogroup" class="@Classname">
        <CascadingValue Value="@this" IsFixed>
            @RadioList
        </CascadingValue>
    </div>
</div>
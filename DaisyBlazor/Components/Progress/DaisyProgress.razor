@namespace DaisyBlazor
@inherits DaisyComponentBase

@if (!Indeterminate)
{
    <progress @ref="@Element" id="@Id" style="@Style" class="@Classname" value="@Value" max="@Max" @attributes="@Attributes" />
}
else
{
    <progress @ref="@Element" id="@Id" style="@Style" class="@Classname" max="@Max" @attributes="@Attributes" />
}

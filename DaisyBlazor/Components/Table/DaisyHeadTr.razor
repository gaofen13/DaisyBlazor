@namespace DaisyBlazor
@inherits DaisyComponentBase

<tr @ref="@Element" id="@Id" style="@Style" class="@TrClass" @attributes="@Attributes">
    @if (MultiSelection)
    {
        <th class="td-checkbox">
            <DaisyCheckbox @bind-Value="@Checked" />
        </th>
    }
    @ChildContent
</tr>
<DaisyModal>
    <ChildContent>@Test</ChildContent>
    <ActionContent>
        <DaisyButton Color="Color.Primary" OnClick="Close">关闭</DaisyButton>
        <DaisyButton Color="Color.Secondary" OnClick="NavigateTo">跳转</DaisyButton>
    </ActionContent>
</DaisyModal>

@code {
    [Inject]
    private NavigationManager? Navigation { get; set; }

    [Inject]
    private ModalService ModalService { get; set; } = default!;

    [Parameter]
    public string? Test { get; set; }

    private void NavigateTo()
    {
        Navigation?.NavigateTo("code");
    }

    private void Close()
    {
        ModalService.Close(ModalResult.Ok<string>("test"));
    }
}